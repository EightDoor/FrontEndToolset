{
  "name": "vite-project",
  "version": "0.0.1",
  "main": "./electron/main.js",
  "scripts": {
    "z": "concurrently  \"npm run start\" \"npm run dev\"",
    "start": "wait-on -c waitOnConfig.json http-get://localhost:9999/ && electron ./electron",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vue-tsc --noEmit --skipLibCheck && vite build",
    "serve": "vite preview",
    "lint": "yarn prettier --write",
    "pack": "electron-builder --dir",
    "dist:all": "cross-env NODE_ENV=production electron-builder -mwl",
    "dist:mac": "cross-env NODE_ENV=production electron-builder -m",
    "dist:win": "cross-env NODE_ENV=production electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps",
    "dist:linux": "electron-builder --linux deb tar.xz AppImage",
    "prepare": "husky install"
  },
  "author": "周凯 <851708184@qq.com>",
  "dependencies": {
    "@amap/amap-jsapi-types": "^0.0.8",
    "@element-plus/icons": "^0.0.11",
    "axios": "^0.21.4",
    "codemirror": "^5.63.3",
    "electron-dl": "^3.2.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "element-plus": "^1.2.0-beta.1",
    "highlight.js": "^11.2.0",
    "json-to-ts": "^1.7.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/codemirror": "^5.60.2",
    "@types/localforage": "^0.0.34",
    "@types/lodash": "^4.14.172",
    "@types/md5": "^2.3.1",
    "@types/node": "^16.7.8",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.1.5",
    "chalk": "^4.1.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "debugout.js": "^1.0.0",
    "electron": "^15.3.0",
    "electron-builder": "22.13.1",
    "electron-rebuild": "^3.2.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.2",
    "less": "^4.1.1",
    "prettier": "2.3.2",
    "typescript": "^4.4.4",
    "vite": "^2.6.7",
    "vue-tsc": "^0.28.4",
    "wait-on": "^6.0.0"
  },
  "build": {
    "productName": "个人工具箱",
    "appId": "cn.start6",
    "copyright": "周凯",
    "compression": "store",
    "publish": [
      {
        "provider": "generic",
        "url": "https://xxx.com"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "images/tap.ico"
    },
    "mac" : {
      "target": ["dmg"]
    },
    "dmg" : {
      "icon": "images/tap.ico",
      "title": "个人工具箱"
    },
    "nsis": {
      "oneClick": false,
      "language": "2052",
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "createDesktopShortcut": true,
      "runAfterFinish": true
    }
  },
  "__npminstall_done": false
}
